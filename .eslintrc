{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-shadow": [
      2,
      {
        "builtinGlobals": false,
        "hoist": "functions",
        "allow": [
          "setBookmark",
          "logoutUser",
          "loginUser",
          "clearErrors",
          "registerUser",
          "updateUserWords",
          "setKnowWord",
          "setActive",
          "setDeleted"
        ]
      }
    ],
    "react/no-unescaped-entities": 0,
    "react/jsx-props-no-spreading": 0,
    "no-console": [
      "error",
      {
        "allow": [
          "error"
        ]
      }
    ],
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "react/prop-types": 0,
    "no-underscore-dangle": 0,
    "import/imports-first": [
      "error",
      "absolute-first"
    ],
    "import/newline-after-import": "error",
    "import/prefer-default-export": 0,
    "semi": "error"
  },
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
    "FormData": true,
    "FileReader": true,
    "Blob": true,
    "navigator": true,
    "Headers": true,
    "Request": true,
    "fetch": true
  }
  //  "parser": "babel-eslint"
}